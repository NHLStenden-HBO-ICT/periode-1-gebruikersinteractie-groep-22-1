<Page x:Class="Slime_Busters.WinkelPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Slime_Busters"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="WinkelPage">

    <Grid Height="1080px" Width="1920px">
        <Rectangle Width="1920" Height="1080" Fill= "Orange" />
        <TextBlock Text="Winkel Page" FontSize="100" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button x:Name="BackButton" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10" Click="BackButtonClick"/>
        <Frame Name="Winkel" NavigationUIVisibility="Hidden" Height="1080px" Width="1920px"></Frame>
    </Grid>
</Page>
